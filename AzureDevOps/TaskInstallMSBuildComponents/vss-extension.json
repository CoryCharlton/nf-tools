{
    "manifestVersion": 1,
    "id": "install-nanoframework-msbuild-components",
    "name": "Install .NET nanoFramework MSBuild components",
    "version": "1.228.0",
    "publisher": "nanoframework",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Install .NET nanoFramework MSBuild components allowing building a .NET nanoFramework C# application with msbuild.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "extension-icon.png"        
    }, 
    "tags": [
        "nanoframework",
        "msbuild",
        "build"
    ],
    "files": [
        {
            "path": "TaskFiles"
        }
    ],
    "contributions": [
        {
            "id": "install-nanoframework-msbuild-components",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "TaskFiles"
            }
        }
    ],
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "links": {
        "home": {
            "uri": "https://www.nanoframework.net"
        },
        "support": {
            "uri": "https://discord.gg/gCyBu8T"
        },
        "repository": {
            "uri": "https://github.com/nanoframework/nf-tools"
        },
        "issues": {
            "uri": "https://github.com/nanoframework/Home/issues"
        }
    },  
    "repository": {
        "type": "git",
        "uri": "https://github.com/nanoframework/nf-tools"
    },    
    "galleryFlags": [
        "Public"
    ],
    "CustomerQnASupport": {
        "enableqna":"true"
    }
}
